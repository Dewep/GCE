{
  "name": "gce",
  "version": "2.0.0",
  "description": "Global Commands Execution",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dewep/GCE.git"
  },
  "author": "Maigret Aur√©lien",
  "license": "SEE LICENSE IN LICENCE",
  "scripts": {
    "start": "electron .",
    "dev": "brunch watch",
    "watch": "nodemon --watch app -e js,vue,html,scss --exec brunch build",
    "build": "brunch build -p",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "net.dewep.gce",
    "productName": "GCE",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": "AppImage",
      "category": "Development"
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "dependencies": {
    "electron-window-state": "5.0.2",
    "fix-path": "2.1.0",
    "js-yaml": "3.11.0",
    "tree-kill": "1.2.0",
    "vue": "2.5.17",
    "vue-router": "3.0.1",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "brunch": "2.10.17",
    "electron": "3.0.5",
    "electron-builder": "20.28.4",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "nodemon": "^1.18.5",
    "sass-brunch": "2.10.7",
    "vue-brunch": "2.0.3"
  }
}
